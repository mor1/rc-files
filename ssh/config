## global

# mitigate CVE-2016-0778 and CVE-2016-0777
UseRoaming no

## cucl
Host armyofdockerness.cl
     User mort
     HostName %h.cam.ac.uk
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent yes

Host *.cl
     User rmm1002
     HostName %h.cam.ac.uk
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent yes
     ProxyCommand ~/.ssh/ssh-cucl-proxy %h %p

Host tfc-app2.cl.cam.ac.uk
     User rita
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent no

Host gitlab.srg.cl.cam.ac.uk
     User git
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent no

Host gitlab-shell.srg svr-rmm1002-git.cl svr-rmm1002-git.cl.cam.ac.uk
     Hostname svr-rmm1002-git.cl.cam.ac.uk
     User rmm1002
     Port 2222
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent no
     # ProxyCommand ~/.ssh/ssh-cucl-proxy %h %p

Host *.cl.cam.ac.uk linux.cl.ds.cam.ac.uk linux.ds.cam.ac.uk
     User rmm1002
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent yes
     StrictHostKeyChecking no
     UserKnownHostsFile=/dev/null

