## global

# mitigate CVE-2016-0778 and CVE-2016-0777
UseRoaming no

## cucl
Host armyofdockerness.cl
     User mort
     HostName %h.cam.ac.uk
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent yes

Host tfc-app2.cl.cam.ac.uk
     User rita
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent no

Host gitlab.srg.cl.cam.ac.uk
     User git
     Port 2222
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent no

Host gitlab-shell.srg.cl.cam.ac.uk
     User rmm1002
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent no

Host *.cl
     User rmm1002
     HostName %h.cam.ac.uk
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent yes
     ProxyCommand ~/.ssh/ssh-cucl-proxy %h %p

Host *.cl.cam.ac.uk
     User rmm1002
     IdentityFile ~/.ssh/cucl-key
     ForwardX11 no
     ForwardAgent yes

